{
  "name": "sulcalc",
  "version": "0.5.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.app.config.js --env.development --https --content-base dist/app",
    "build:app": "webpack --config build/webpack.app.config.js --env.production",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style": "prettier '**/*.{js,json,html,md,scss,vue,yml,yaml}' --ignore-path .eslintignore",
    "style:fix": "npm run style -- --write",
    "setup": "npm run setup:db && npm run setup:stats && npm run setup:setdex",
    "setup:db": "babel-node scripts/db",
    "setup:stats": "babel-node scripts/stats",
    "setup:setdex": "babel-node scripts/setdex"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{js,json,html,md,scss,vue,yml,yaml}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && jest -o",
      "pre-push": "npm run setup && npm run lint && npm test"
    }
  },
  "dependencies": {
    "big-integer": "^1.6.36",
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-multiselect": "^2.1.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0-beta.4",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "terser-webpack-plugin": "^1.1.0",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
