{
  "name": "sulcalc",
  "version": "0.4.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "serve": "webpack-dev-server --config build/webpack.app.dev.js",
    "build": "npm run build:app:production",
    "build:app": "webpack --config build/webpack.app.dev.js",
    "build:app:production": "webpack --config build/webpack.app.prod.js",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style:fix": "npm run style -- --write",
    "style": "prettier '**/*.{js,json,md,vue}' --ignore-path .eslintignore",
    "setup": "npm run db && npm run setdex",
    "db": "babel-node scripts/db",
    "setdex": "babel-node scripts/setdex"
  },
  "dependencies": {
    "big-integer": "^1.6.26",
    "bootstrap": "^4.0.0",
    "lodash": "^4.17.5",
    "vue": "^2.5.13",
    "vue-multiselect": "^2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.1",
    "prettier": "^1.10.2",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.20.1",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
