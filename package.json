{
  "name": "sulcalc",
  "version": "0.5.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "start": "webpack-serve --config build/webpack.app.config.js --no-clipboard",
    "build:app": "webpack --config build/webpack.app.config.js",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style": "prettier '**/*.{js,json,md,vue,yml,yaml}' --ignore-path .eslintignore",
    "style:fix": "npm run style -- --write",
    "setup": "npm run setup:db && npm run setup:stats && npm run setup:setdex",
    "setup:db": "babel-node scripts/db",
    "setup:stats": "babel-node scripts/stats",
    "setup:setdex": "babel-node scripts/setdex"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{js,json,md,vue,yml,yaml}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "big-integer": "^1.6.36",
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-multiselect": "^2.1.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "prettier": "^1.14.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "terser-webpack-plugin": "^1.1.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2"
  }
}
