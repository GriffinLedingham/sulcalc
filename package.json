{
  "name": "sulcalc",
  "version": "0.6.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.app.config.js --env.development",
    "build:app": "webpack --config build/webpack.app.config.js --env.production",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "jest --coverage",
    "lint": "tsc && tslint -p . && npm run style -- --list-different",
    "style": "prettier '**/*.{js,json,html,md,scss,ts,vue,yml,yaml}'",
    "style:fix": "npm run style -- --write",
    "setup": "npm run setup:db && npm run setup:stats && npm run setup:setdex",
    "setup:db": "babel-node scripts/db --extensions '.js,.ts'",
    "setup:stats": "babel-node scripts/stats --extensions '.js,.ts'",
    "setup:setdex": "babel-node scripts/setdex --extensions '.js,.ts'"
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint"
    ],
    "*.{js,json,html,md,scss,ts,vue,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && jest -o",
      "pre-push": "npm run setup && npm run lint && npm test"
    }
  },
  "dependencies": {
    "big-integer": "^1.6.42",
    "bootstrap": "^4.3.1",
    "lodash": "^4.17.11",
    "vue": "^2.6.7",
    "vue-multiselect": "^2.1.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.121",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^2.1.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "terser-webpack-plugin": "^1.2.3",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "vue-jest": "^3.0.3",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "workbox-webpack-plugin": "^4.0.0"
  }
}
