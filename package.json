{
  "name": "sulcalc",
  "version": "0.4.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "serve":
      "webpack-serve --config build/webpack.app.config.js --no-clipboard",
    "build": "webpack --config build/webpack.app.config.js",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style:fix": "npm run style -- --write",
    "style": "prettier '**/*.{js,json,md,vue}' --ignore-path .eslintignore",
    "setup": "npm run db && npm run setdex",
    "db": "babel-node scripts/db",
    "setdex": "babel-node scripts/setdex"
  },
  "dependencies": {
    "big-integer": "^1.6.27",
    "bootstrap": "^4.1.0",
    "lodash": "^4.17.5",
    "vue": "^2.5.16",
    "vue-multiselect": "2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.44",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.44",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-promise": "^3.7.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "prettier": "^1.11.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-serve": "^0.3.1"
  }
}
